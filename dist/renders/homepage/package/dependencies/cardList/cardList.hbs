<div class="amp-ca-card-list
{{#iff cards.length '<' 6}}amp-ca-prod-{{cards.length}}-rows
{{else}}amp-ca-prod-3-rows{{/iff}}
{{#if heroList}}amp-ca-cards-hero{{/if}}
">
    <div class="amp-ca-card-title">
        {{ header }}
    </div>

    <div class="amp-ca-card-list-wrap">
        {{#each cards}}
            {{dynamicTemplate [@type] ../renderTypes this @index}}
        {{/each}}
    </div>
</div>